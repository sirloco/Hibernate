<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="accesodatos.Gestion" table="gestion" schema="gestionpruebas">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="codproveedor">
            <column name="codproveedor" sql-type="varchar(6)" length="6"/>
        </property>
        <property name="codpieza">
            <column name="codpieza" sql-type="varchar(6)" length="6"/>
        </property>
        <property name="codproyecto">
            <column name="codproyecto" sql-type="varchar(6)" length="6"/>
        </property>
        <property name="cantidad">
            <column name="cantidad" sql-type="float" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>